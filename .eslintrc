{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "vue/html-indent": [
      "error",
      "tab"
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}